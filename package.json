{
  "name": "@rask/root",
  "description": "Rask Web Components Monorepo",
  "author": "Dustin Griffith",
  "version": "0.0.1",
  "private": true,
  "license": "MIT",
  "packageManager": "pnpm@8.2.0",
  "engines": {
    "node": ">=18.11.0 || >=19.2.0",
    "pnpm": ">=8.2.0"
  },
  "type": "module",
  "workspaces": [
    "api/",
    "apps/*",
    "packages/*"
  ],
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "6.0.0-alpha.136",
    "@typescript-eslint/parser": "6.0.0-alpha.136",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-lit": "^1.8.3",
    "eslint-plugin-prettier": "^4.2.1",
    "pnpm": "^8.3.1",
    "prettier": "^2.8.8",
    "prettier-plugin-multiline-arrays": "^1.1.3"
  },
  "pnpm": {
    "overrides": {
      "@rask/core": "workspace:*",
      "@rask/web": "workspace:*",
      "typescript": "5.1.0-beta"
    },
    "peerDependencyRules": {
      "allowAny": [
        "@types/node"
      ],
      "ignoreMissing": [
        "typescript"
      ]
    }
  }
}
