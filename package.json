{
  "name": "@rask/root",
  "description": "Rask Web Components Monorepo",
  "author": "Dustin Griffith",
  "version": "0.0.1",
  "private": true,
  "packageManager": "pnpm@8.1.0",
  "engines": {
    "node": ">=16",
    "pnpm": ">=8.0.0"
  },
  "type": "module",
  "devDependencies": {
    "pnpm": "^8.1.0",
    "rome": "12.0.0"
  },
  "pnpm": {
    "overrides": {
      "@rask/core": "workspace:*",
      "@rask/web": "workspace:*",
      "typescript": "5.0.3"
    },
    "peerDependencyRules": {
      "allowAny": ["@types/node"],
      "ignoreMissing": ["typescript"]
    }
  }
}
