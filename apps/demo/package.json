{
  "name": "@rask/demo",
  "private": true,
  "version": "0.0.1",
  "packageManager": "pnpm@8.2.0",
  "engines": {
    "node": ">=18.11.0 >=19.2.0",
    "pnpm": ">=8.2.0"
  },
  "type": "module",
  "imports": {
    "#/*": "./src/**/*.js"
  },
  "files": [
    "src/**/*.{map.js,js,css}",
    "types/src/**/*.d.ts",
    "README.md"
  ],
  "exports": {
    "./client": {
      "types": "./src/types/client.d.ts"
    },
    "./src/client/*": "./src/client/*"
  },
  "scripts": {
    "codegen": "graphql-codegen --config codegen.yml",
    "codegen:watch": "pnpm codegen -w",
    "dev": "wds"
  },
  "dependencies": {
    "@apollo/client": "^3.7.12",
    "@lit-labs/context": "^0.3.0",
    "@lit-labs/task": "^2.1.0",
    "@lit/reactive-element": "^1.6.1",
    "@material/web": "1.0.0-pre.6",
    "@rask/core": "workspace:^",
    "@rask/graphql": "workspace:^",
    "@rask/identity": "workspace:^",
    "@rask/web": "workspace:*",
    "@vaadin/router": "^1.7.5",
    "graphql": "^16.6.0",
    "lit": "^2.7.2",
    "tslib": "^2.5.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^3.3.0",
    "@graphql-codegen/typed-document-node": "^4.0.0",
    "@graphql-codegen/typescript": "^3.0.3",
    "@graphql-codegen/typescript-operations": "^3.0.3",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@rollup/plugin-graphql": "^2.0.3",
    "@types/node": "^18.15.11",
    "@web/dev-server": "^0.1.38",
    "@web/dev-server-esbuild": "^0.3.6",
    "@web/dev-server-hmr": "^0.1.12",
    "@web/dev-server-rollup": "^0.4.1",
    "graphql-tag": "^2.12.6",
    "postcss-preset-env": "^8.3.2",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-typescript-paths": "^1.4.0",
    "typescript": "5.0.4"
  }
}
