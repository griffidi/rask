{
  "name": "@rask/api",
  "private": true,
  "version": "0.0.1",
  "packageManager": "pnpm@8.2.0",
  "engines": {
    "node": ">=18.11.0 >=19.2.0",
    "pnpm": ">=8.2.0"
  },
  "type": "module",
  "files": [
    "src/**/*.{map.js,js}",
    "README.md"
  ],
  "scripts": {
    "preprisma:generate": "rimraf prisma/dev.db",
    "prisma:generate": "prisma generate",
    "postprisma:generate": "cd $PWD && cd ./node_modules/@prisma/generated/type-graphql && pnpm init",
    "prisma:push": "prisma db push",
    "prisma:seed": "prisma db seed",
    "generate:db": "pnpm prisma:generate && pnpm prisma:push && pnpm prisma:seed",
    "dev": "ts-node-esm --transpile-only src/index.ts",
    "build": ""
  },
  "dependencies": {
    "@apollo/server": "^4.6.0",
    "@apollo/utils.keyvaluecache": "^2.1.1",
    "@as-integrations/koa": "^0.3.0",
    "@koa/cors": "^4.0.0",
    "@prisma/client": "^4.12.0",
    "dotenv": "^16.0.3",
    "graphql": "^16.6.0",
    "graphql-fields": "^2.0.3",
    "graphql-scalars": "^1.21.3",
    "koa": "^2.14.2",
    "koa-bodyparser": "^4.4.0",
    "reflect-metadata": "^0.1.13",
    "tslib": "^2.5.0",
    "type-graphql": "2.0.0-beta.1"
  },
  "devDependencies": {
    "@ngneat/falso": "^6.4.0",
    "@types/graphql-fields": "^1.3.5",
    "@types/koa": "^2.13.6",
    "@types/koa-bodyparser": "^4.3.10",
    "@types/koa__cors": "^4.0.0",
    "@types/node": "^18.15.11",
    "class-validator": "^0.14.0",
    "graphql-tag": "^2.12.6",
    "nanoid": "^4.0.2",
    "prisma": "^4.12.0",
    "rimraf": "^5.0.0",
    "ts-node": "^10.9.1",
    "typegraphql-prisma": "0.24.5",
    "typescript": "5.0.4"
  },
  "peerDependencies": {
    "typescript": "5.*"
  },
  "prisma": {
    "seed": "ts-node-esm --transpile-only prisma/seed.ts"
  }
}
