{
  "name": "@rask/core",
  "private": true,
  "version": "0.0.1",
  "packageManager": "pnpm@8.1.0",
  "engines": {
    "node": ">=16",
    "pnpm": ">=8.0.0"
  },
  "type": "module",
  "imports": {
    "#/util/*.js": "./src/util/*.js"
  },
  "exports": {
    ".": {
      "types": "./types/src/index.d.ts",
      "import": "./dist/index.js"
    },
    "./cache/index.js": {
      "types": "./types/src/cache/index.d.ts",
      "import": "./dist/cache/index.js"
    },
    "./crypto/index.js": {
      "types": "./types/src/crypto/index.d.ts",
      "import": "./dist/crypto/index.js"
    },
    "./di/index.js": {
      "types": "./types/src/di/index.d.ts",
      "import": "./dist/di/index.js"
    },
    "./events/index.js": {
      "types": "./types/src/events/index.d.ts",
      "import": "./dist/events/index.js"
    },
    "./reactive/signals/index.js": {
      "types": "./types/src/reactive/signals/index.d.ts",
      "import": "./dist/reactive/signals/index.js"
    },
    "./reactive/timer/index.js": {
      "types": "./types/src/reactive/timer/index.d.ts",
      "import": "./dist/reactive/timer/index.js"
    },
    "./types/*.js": {
      "types": "./types/src/types/*.d.ts",
      "import": "./dist/types/*.js"
    },
    "./validation/index.js": {
      "types": "./types/src/validation/index.d.ts",
      "import": "./dist/validation/index.js"
    }
  },
  "files": [
    "dist/**/*.{map.js,js}",
    "types/src/**/*.d.ts",
    "README.md",
    "!types/tsconfig.tsbuildinfo"
  ],
  "scripts": {
    "dev": "",
    "build": "tsc --pretty"
  },
  "devDependencies": {
    "typescript": "^5.0.3"
  },
  "peerDependencies": {
    "typescript": "5.0.3"
  },
  "dependencies": {
    "tslib": "^2.5.0"
  }
}
